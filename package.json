{
    "jest": {
        "verbose": true,
        "testEnvironment": "jsdom",
        "moduleNameMapper": {
            "^App(.*)": "<rootDir>/assets/js/react/App/$1",
            "^Assets(.*)": "<rootDir>/assets/js/assets.js$1",
            "^Error(.*)": "<rootDir>/assets/js/react/Error/$1",
            "^Hoc(.*)": "<rootDir>/assets/js/react/Hoc/$1",
            "^Services(.*)": "<rootDir>/assets/js/react/Services/$1",
            "^Templates(.*)": "<rootDir>/assets/js/react/Templates/$1",
            "^Widgets(.*)": "<rootDir>/assets/js/react/Widgets/$1",
            "^InfoManual(.*)": "<rootDir>/assets/js/react/InfoManual/$1",
